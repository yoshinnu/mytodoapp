<!doctype html>
<html>

<head>
  <%- include("./share/stylesheets.ejs") %>
  <meta charset=" utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="generator" content="Jekyll v3.8.5">
  <title><%= title %></title>

  <style>
    .bd-placeholder-img {
      font-size: 1.125rem;
      text-anchor: middle;
    }

    @media (min-width: 768px) {
      .bd-placeholder-img-lg {
        font-size: 3.5rem;
      }
    }
  </style>
  <!-- Custom styles for this template -->
</head>

<body>
  <%- include("./share/bodyheader.ejs") %>
  <main role="main">
    <section class="jumbotron text-center">
      <div class="container">

        <h1 class="jumbotron-heading"><%= title %></h1>
        <p><%= user.username %></p>
        <p>
          <a href="#" class="btn btn-primary my-2">Doing Todolist</a>
          <a href="#" class="btn btn-secondary my-2">Complite Todolist</a>
          <a href="/home/todo" class="btn btn-secondary my-2">Create Todo</a>
        </p>
      </div>
    </section>

    <div class="album py-5 bg-light">
      <div class="container">
        <div class="row" id="todobox">
        </div>
      </div>
    </div>
  </main>
  <%- include("./share/footer.ejs") %>
  <%- include("./share/javascript.ejs") %>
  <script src="/public/javascripts/todolist.js"></script>
</body>

</html>